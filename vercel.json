{
    "version": 2,
    "builds": [
      {
        "src": "main.js",
        "use": "@vercel/node"
      },
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/node/(.*)",   
        "dest": "/main.js"
      },
      {
        "src": "/python/(.*)",  
        "dest": "/app.py"
      }
    ]
  }
  